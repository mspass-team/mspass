      <memberdef kind="function" id="classmspass_1_1algorithms_1_1deconvolution_1_1_least_square_decon_1aff1ef8576f2d5890f0a7fd50797971e9" prot="public" static="no" const="no" explicit="no" inline="no" virt="virtual">
